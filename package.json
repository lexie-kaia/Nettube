{
  "name": "nettube",
  "version": "1.0.0",
  "description": "Youtube clone coding projects built with Node.js and MongoDB",
  "main": "index.js",
  "repository": "https://github.com/lexie-kaia/Nettube.git",
  "author": "lexie-kaia <kaia.gayeon.kim@gmail.com>",
  "private": true,
  "scripts": {
    "start": "node build/init.js",
    "prebuild": "rm -rf build && rm -rf src/static",
    "build": "yarn build:server && yarn build:assets && yarn copy:assets",
    "build:server": "babel src --out-dir build --ignore src/assets/**/*.js --ignore src/static/*.js",
    "build:assets": "webpack --mode=production",
    "copy:assets": "cp -r src/static build && cp -r src/views build",
    "dev:server": "nodemon --exec babel-node src/init.js",
    "dev:assets": "webpack --mode=development --watch"
  },
  "dependencies": {
    "aws-sdk": "^2.852.0",
    "axios": "^0.21.1",
    "connect-mongo": "3.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "helmet": "^4.4.1",
    "mongoose": "^5.11.17",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-github2": "^0.1.12",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.1.0",
    "pug": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.12.16",
    "@babel/node": "^7.12.16",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.16",
    "autoprefixer": "^10.2.4",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.2",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "mini-css-extract-plugin": "^1.3.8",
    "nodemon": "^2.0.7",
    "postcss": "^8.2.6",
    "postcss-loader": "^5.0.0",
    "prettier": "^2.2.1",
    "sass": "^1.32.7",
    "sass-loader": "^11.0.1",
    "webpack": "^5.23.0",
    "webpack-cli": "^4.5.0"
  },
  "browserslist": [
    "last 2 versions"
  ]
}
